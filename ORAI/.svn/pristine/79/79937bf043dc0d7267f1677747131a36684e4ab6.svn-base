package cmn;

/**
 * @author SIST
 *
 */
public class DTO {
	private int    total     ;   //총글수 
	private int    num       ;   //글번호
	private int wFlag; //flag  
	
	
	public DTO(){}


	@Override
	public String toString() {
		return "DTO [total=" + total + ", num=" + num + ", wFlag=" + wFlag + "]";
	}


	public DTO(int total, int num, int wFlag) {
		super();
		this.total = total;
		this.num = num;
		this.wFlag = wFlag;
	}


	public int getTotal() {
		return total;
	}


	public void setTotal(int total) {
		this.total = total;
	}


	public int getNum() {
		return num;
	}


	public void setNum(int num) {
		this.num = num;
	}


	public int getwFlag() {
		return wFlag;
	}


	public void setwFlag(int wFlag) {
		this.wFlag = wFlag;
	}
	
	
	

	
	
	
}
